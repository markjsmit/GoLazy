<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.noobsunited.goLazy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GoLazyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.noobsunited.goLazy.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.noobsunited.goLazy.activities.Auth.GoogleAuth" />
        <activity android:name="com.noobsunited.goLazy.activities.Auth.PtcAuth" />
        <activity
            android:name="com.noobsunited.goLazy.activities.mainMenuActivity"
            android:label="@string/title_activity_base"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>